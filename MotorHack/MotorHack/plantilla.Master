                    </div>
                    <div id="dvRegistrarse">
                         <h2 class="text-center"><img src="Imagenes/Escudo/MH.png" class="img-circle"><br>Registrarse</h2>
                        <h6 class="text-center">Completa los siguientes campos</h6>
                        <div class="form-group">
                            <asp:TextBox ID="txtUsuario"  class="form-control input-lg"  placeholder="Nickname" runat="server"></asp:TextBox>
                        </div>
                        <div class="form-group">
                            <asp:TextBox ID="txtContrasena"  class="form-control input-lg"  MaxLength="15" placeholder="Contraseña" TextMode="Password" runat="server"></asp:TextBox>

                        </div>
                        <div class="form-group">
                            <asp:TextBox ID="txtConfContra"  class="form-control input-lg" MaxLength="15" placeholder="Confirmar Contraseña" TextMode="Password" runat="server"></asp:TextBox>
                        </div>
                        <div class="form-group">
                            <asp:TextBox ID="txtCorreo"  class="form-control input-lg" placeholder="Correo Electrónico" runat="server"></asp:TextBox>
                        </div>
                        <div class="form-group">
                            <asp:TextBox ID="txtNombreCompleto"  class="form-control input-lg" placeholder="Nombre Completo" runat="server"></asp:TextBox>
                        </div>
                        <div class="form-group">
                            <asp:TextBox ID="txtRFC"   class="form-control input-lg" MaxLength="13" placeholder="RFC" styles="text-transform: uppercase"  runat="server"></asp:TextBox>
                        </div>
                        <div class="form-group">

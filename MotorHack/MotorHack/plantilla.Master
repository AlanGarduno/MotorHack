    <script type="text/javascript">
        jQuery(function () {
            jQuery("#dvRegistrarse").hide();
            jQuery(".bujias").colorbox({ rel: 'bujias' });
            jQuery(".filtrosa").colorbox({ rel: 'filtrosa' });
            jQuery(".filtrosaire").colorbox({ rel: 'filtrosaire' });
            jQuery(".aceites").colorbox({ rel: 'aceites' });
            jQuery(".llantas").colorbox({ rel: 'llantas' });
            jQuery(".rines").colorbox({ rel: 'rines' });
            jQuery(".calipers").colorbox({ rel: 'calipers' });
            jQuery(".amort").colorbox({ rel: 'amort' });
            jQuery(".susp").colorbox({ rel: 'susp' });
            jQuery(".anticon").colorbox({ rel: 'anticon' });
            jQuery(".bgas").colorbox({ rel: 'bgas' });

        });

        function fnRegistrarse() {



            jQuery("#dvRegistrarse").show('slow');
            jQuery("#dvInicioSesion").hide('slow');

            jQuery("#txtEmail").removeProp("required");
            jQuery("#txtPassword").removeProp("required");

            jQuery("#txtUsuario").prop("required", "required");
            jQuery("#txtContrasena").prop("required", "required");
            jQuery("#txtConfContra").prop("required", "required");
            jQuery("#txtCorreo").prop("required", "required");
            jQuery("#txtNombreCompleto").prop("required", "required");
            jQuery("#txtRFC").prop("required", "required");
        }
        function fnRegresar() {
            jQuery("#dvRegistrarse").hide('slow');
            jQuery("#dvInicioSesion").show('slow');

            jQuery("#txtEmail").prop("required", "required");
            jQuery("#txtPassword").prop("required", "required");

            jQuery("#txtUsuario").removeProp("required");
            jQuery("#txtContrasena").removeProp("required");
            jQuery("#txtConfContra").removeProp("required");
            jQuery("#txtCorreo").removeProp("required");
            jQuery("#txtNombreCompleto").removeProp("required");
            jQuery("#txtRFC").removeProp("required");
        }

    </script>
      
      
             
